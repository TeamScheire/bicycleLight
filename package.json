{
    "name": "be.postduif.bicyclelight",
    "displayName": "bicycleLight",
    "version": "1.0.0",
    "description": "A companion app for the bluetooth smart bicycle light",
    "main": "app.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "Apache Cordova Team",
    "license": "Apache-2.0",
    "dependencies": {
        "cordova-ios": "^4.5.5",
        "cordova-plugin-ble-central": "^1.2.0",
        "cordova-plugin-compat": "^1.2.0",
        "cordova-plugin-geolocation": "^4.0.1",
        "cordova-plugin-mauron85-background-geolocation": "^2.3.5",
        "cordova-plugin-whitelist": "^1.3.3",
        "onsenui": "^2.10.4"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-plugin-ble-central": {
                "TEXT": "Connecting to a ble bicycle bell."
            },
            "cordova-plugin-geolocation": {
                "GEOLOCATION_USAGE_DESCRIPTION": "To locate you"
            },
            "cordova-plugin-mauron85-background-geolocation": {
                "ALWAYS_USAGE_DESCRIPTION": "This app always requires location tracking"
            }
        },
        "platforms": [
            "ios"
        ]
    }
}